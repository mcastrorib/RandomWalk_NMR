---- PFGSE CONFIGURATION
-- Physical attributes.
D0: 2.5
GIROMAGNETIC_RATIO: 42.576
PULSE_WIDTH: 0.1
MAX_GRADIENT: {3000.0, 0.0, 0.0}
GRADIENT_SAMPLES: 200

-- Time sequence.
- TIME_SEQ 		== can be 'manual', linear', 'log'. 
TIME_SEQ: log
TIME_SAMPLES: 3
//TIME_VALUES: {0.2, 0.5, 1.0, 2.0}
TIME_MIN: -2.0
TIME_MAX: 0.3
APPLY_SCALE_FACTOR: true
INSPECTION_LENGTH: 100.0

-- Threshold application for D(t) recovering.
- TIME_TYPE 	== can be 'samples', lhs', 'none'. 
THRESHOLD_TYPE: samples
THRESHOLD_VALUE: 8

-- Wave-vector 'k' computation.
USE_WAVEVECTOR_TWOPI: false
ALLOW_WALKER_SAMPLING: true

-- PFGSE SAVE. 
SAVE_MODE: true
SAVE_PFGSE: true
SAVE_COLLISIONS: false
SAVE_DECAY: false
SAVE_HISTOGRAM: true
SAVE_HISTOGRAM_LIST: false
